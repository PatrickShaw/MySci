<UserControl x:Class="MyChem_Program.Modules.Calculators.ppmBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignWidth="300" Height="85.143">
    <UserControl.Resources>
        <ResourceDictionary Source="/MyChem Program;component/Styles.xaml"></ResourceDictionary>
    </UserControl.Resources>
    <Grid x:Name="grid">
        <Label Content="Parts per million" FontWeight="Bold" FontSize="15" HorizontalAlignment="Left" Margin="10,11,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtPPM" Height="28" Margin="10,46,10,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
